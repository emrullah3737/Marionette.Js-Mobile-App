{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-new": 1,
        "class-methods-use-this": 1,
        "no-useless-constructor": 1,
        "consistent-return": 1,
        "no-restricted-syntax": 1,
        "no-unused-vars": 1,
        "no-undef": 1,
        "no-lonely-if": 1,
        "prefer-rest-params": 1,
        "no-underscore-dangle": 1,
        "camelcase": 1
    }
}
